<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.221" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.CaseViewController">
   <children>
      <ListView fx:id="caseList" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <HBox layoutY="666.0" prefHeight="35.0" prefWidth="330.0">
         <children>
            <Button fx:id="addCaseButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="Dodaj" textAlignment="JUSTIFY" HBox.hgrow="ALWAYS" />
            <Button fx:id="archiveCaseButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="Zarchiwizuj" HBox.hgrow="ALWAYS" />
            <ToggleButton fx:id="editCaseToggleButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="Edytuj" HBox.hgrow="ALWAYS" />
            <Button fx:id="linkCaseButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="Powiąż" HBox.hgrow="ALWAYS" />
            <Button fx:id="deleteCaseButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="Usuń" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</AnchorPane>
